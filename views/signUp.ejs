<div>
    <h2>Sign-up</h2><br><br>

    <form method="POST" action="/sign-up/create">
        <input name="first" placeholder="First Name" type="text" required>
        <input name="last" placeholder="Last Name" type="text" required>
        <input name="email" placeholder="E-mail" type="text" required>
        <input name="password" placeholder="password" type="password" required>
        <input type="submit" value="Submit">
    </form>
</div>